{
  "name": "@security-alert/share",
  "version": "1.1.1",
  "description": "[GitHub] Create an issue from security alert's url.",
  "keywords": [
    "cli",
    "console",
    "github",
    "issue",
    "security",
    "tool"
  ],
  "homepage": "https://github.com/azu/security-alert",
  "bugs": {
    "url": "https://github.com/azu/security-alert/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/security-alert.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "watch": "tsc -p . --watch"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "devDependencies": {
    "@types/lodash": "^4.14.134",
    "@types/meow": "^5.0.0",
    "@types/mocha": "^5.2.7",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.0.10",
    "cross-env": "^5.2.0",
    "husky": "^2.5.0",
    "lint-staged": "^8.2.1",
    "mocha": "^6.2.0",
    "nock": "^10.0.6",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "ts-node-test-register": "^8.0.1",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@npm/types": "^1.0.1",
    "@octokit/graphql": "^2.1.2",
    "@octokit/rest": "^16.28.2",
    "@yarnpkg/lockfile": "^1.0.0",
    "meow": "^5.0.0"
  },
  "gitHead": "ad7914038cd53ca2b0676071f9da572692d6e171",
  "publishConfig": {
    "access": "public"
  }
