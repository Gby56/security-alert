{
  "private": true,
  "name": "security-alert",
  "version": "1.0.0",
  "description": "security-alert monorepo",
  "devDependencies": {
    "lerna": "^3.22.1"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "test": "lerna run test",
    "versionup": "lerna version --conventional-commits --create-release github",
    "versionup:patch": "lerna version patch --conventional-commits --create-release github",
    "versionup:minor": "lerna version minor --conventional-commits --create-release github",
    "versionup:major": "lerna version major --conventional-commits --create-release github",
    "release": "lerna publish from-package",
    "release:canary": "lerna publish --canary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azu/security-alert.git"
  },
  "author": "azu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azu/security-alert/issues"
  },
  "homepage": "https://github.com/azu/security-alert",
  "workspaces": [
    "packages/*"
  ]
}
